{
	"name": "project-acc",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"@types/js-cookie": "^2.2.0",
		"@types/nprogress": "0.0.29",
		"axios": "^0.18.0",
		"element-ui": "^2.4.11",
		"font-awesome": "^4.7.0",
		"js-cookie": "^2.2.0",
		"lodash": "^4.17.11",
		"moment": "^2.22.2",
		"normalize.css": "^8.0.1",
		"nprogress": "^0.2.0",
		"vue": "^2.5.17",
		"vue-class-component": "^6.0.0",
		"vue-property-decorator": "^7.0.0",
		"vue-router": "^3.0.1",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.2.0",
		"@vue/cli-plugin-eslint": "^3.2.0",
		"@vue/cli-plugin-typescript": "^3.2.0",
		"@vue/cli-service": "^3.2.0",
		"@vue/eslint-config-prettier": "^4.0.0",
		"@vue/eslint-config-typescript": "^3.2.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^5.0.0-0",
		"json-loader": "^0.5.7",
		"node-sass": "^4.9.0",
		"sass-loader": "^7.0.1",
		"typescript": "^3.0.0",
		"vue-template-compiler": "^2.5.17"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/typescript"
		],
		"rules": {
			"quotes": [
				"error",
				"single"
			],
			"indent": [
				"error",
				"tab",
				{
					"SwitchCase": 1
				}
			]
		},
		"parserOptions": {
			"parser": "typescript-eslint-parser",
			"ecmaFeatures": {
				"parser": "babel-eslint",
				"jsx": false,
				"globalReturn": true
			}
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
